pipeline {
    agent any

    stages {
        stage('Checkout') {
            steps {
                git branch: 'main', url: 'https://github.com/skarthiselvaraj/Jenkins-Zero-To-Hero'
            }
        }
        // Other stages...
    }
}

        stage('Compile Java') {
            steps {
                // Compile the Java program
                sh 'javac new-folder/HelloWorld.java'
            }
        }
        stage('Run Java') {
            steps {
                // Run the Java program
                sh 'java -cp new-folder HelloWorld'
            }
        }
    }
}
